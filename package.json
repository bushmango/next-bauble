    "barrel-rider-sidecar": "^1.0.23",
    "concurrently": "^5.1.0",
    "d3": "^5.16.0",
    "fast-random": "^2.0.4",
    "isomorphic-unfetch": "3.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "next": "^9.3.5",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
