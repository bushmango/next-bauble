{
  "name": "next-bauble",
  "version": "1.0.0",
  "scripts": {
    "1": "yarn rimraf .next && yarn concurrently \"yarn dev\" \"yarn 2\" \"yarn sidecar\"",
    "2": "yarn css-watch",
    "sidecar": "yarn barrel-rider-sidecar --remove --watch --src components lib",
    "css-watch": "yarn tsm  \"**/*.module.scss\" --ignore \"**/node_modules\" --watch",
    "css": "yarn tsm  \"**/*.module.scss\" --ignore \"**/node_modules\"",
    "dev": "next -p 3080",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "@types/chroma-js": "2.1.3",
    "@types/d3": "^5.7.2",
    "@types/d3-scale-chromatic": "^1.5.0",
    "@types/lodash": "4.14.172",
    "@types/moment": "^2.13.0",
    "@types/victor": "^1.1.0",
    "@zeit/next-sass": "^1.0.1",
    "barrel-rider-sidecar": "1.1.6",
    "canvas": "2.8.0",
    "chroma-js": "2.1.2",
    "concurrently": "6.2.1",
    "d3": "^5.16.0",
    "d3-scale-chromatic": "^1.5.0",
    "fast-random": "^2.0.4",
    "isomorphic-unfetch": "3.1.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "next": "12.1.0",
    "node-sass": "4.14.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "16.7.6",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "husky": "7.0.2",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "typed-scss-modules": "3.0.0",
    "typescript": "4.4.2"
  },
  "license": "UNLICENSED"
}
